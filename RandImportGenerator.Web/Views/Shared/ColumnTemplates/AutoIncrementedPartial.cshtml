
<hr />
<div id="autoIncrementedColumnApp">
    <form v-on:submit.prevent="Submit">
        @Html.Partial("~/Views/Shared/ColumnTemplates/ColumnBasePartial.cshtml")
        <hr />
        <div class="row">
            <div class="col-sm-6">
                <label>Starting Sequence #</label>
                <input type="text" v-model="StartingSequenceNumber" class="form-control inline"/>
            </div>
            <div class="col-sm-6">
                <label>Increment Value</label>
                <input type="text" v-model="IncrementValue" class="form-control inline"/>
            </div>
        </div>
        <div class="row">
            <div class="col-sm-12">
                <button type="submit" class="btn btn-primary">Save</button>
            </div>
        </div>
    </form>
</div>
